{
  "name": "@nodecfdi/credentials",
  "version": "3.1.0",
  "description": "Librer√≠a para usar eFirma (fiel) y CSD (sellos) para el SAT",
  "type": "module",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/esm/types.d.ts",
        "default": "./dist/esm/types.js"
      },
      "require": {
        "types": "./dist/commonjs/types.d.ts",
        "default": "./dist/commonjs/types.js"
      }
    },
    "./package.json": "./package.json",
    "./browser": {
      "import": {
        "types": "./dist/esm/base/index.d.ts",
        "default": "./dist/esm/base/index.js"
      },
      "require": {
        "types": "./dist/commonjs/base/index.d.ts",
        "default": "./dist/commonjs/base/index.js"
      }
    },
    "./node": {
      "import": {
        "types": "./dist/esm/node/index.d.ts",
        "default": "./dist/esm/node/index.js"
      },
      "require": {
        "types": "./dist/commonjs/node/index.d.ts",
        "default": "./dist/commonjs/node/index.js"
      }
    }
  },
  "imports": {
    "#src/*": "./src/*.js"
  },
  "scripts": {
    "prepare": "is-in-ci || husky",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "gen:docs": "typedoc --options typedoc.json",
    "gen:changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s -r 0",
    "changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s && git add CHANGELOG.md",
    "prebuild": "pnpm lint:check && pnpm typecheck",
    "build": "tshy",
    "postbuild": "pnpm run gen:docs && git add docs/*",
    "release": "np",
    "version": "pnpm run build && pnpm run changelog"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@jsprismarine/typedoc-material-theme": "^1.0.4",
    "@nodecfdi/eslint-config": "^1.9.3",
    "@nodecfdi/prettier-config": "^1.1.1",
    "@nodecfdi/tsconfig": "^1.5.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.14.15",
    "@vitest/coverage-istanbul": "^2.0.5",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-define-config": "^2.1.0",
    "husky": "^9.1.4",
    "is-in-ci": "^1.0.0",
    "np": "^10.0.7",
    "prettier": "^3.3.3",
    "tshy": "^3.0.2",
    "typedoc": "^0.26.5",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "vitest-mock-extended": "^2.0.0"
  },
  "dependencies": {
    "@nodecfdi/base-converter": "^1.0.6",
    "@vilic/node-forge": "1.3.2-5",
    "ts-mixer": "^6.0.4"
  },
  "peerDependencies": {
    "@types/luxon": "3.4.2",
    "luxon": "^3.5.0"
  },
  "peerDependenciesMeta": {
    "@types/luxon": {
      "optional": true
    }
  },
  "author": "Fernando Isidro <luffynando@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/nodecfdi/credentials",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodecfdi/credentials.git"
  },
  "bugs": {
    "url": "https://github.com/nodecfdi/credentials/issues"
  },
  "keywords": [
    "certificate",
    "sat",
    "csd",
    "fiel",
    "signing",
    "firmar",
    "factura",
    "rfc",
    "efirma",
    "sellos",
    "pfx"
  ],
  "engines": {
    "node": ">=18 <=22 || ^16"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": "@nodecfdi/prettier-config",
  "packageManager": "pnpm@9.7.0",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "np": {
    "message": "chore(release): :tada: %s",
    "tag": "latest",
    "branch": "main",
    "anyBranch": false,
    "testScript": "test:run"
  },
  "tshy": {
    "selfLink": false,
    "exports": {
      ".": "./src/index.ts",
      "./types": "./src/types.ts",
      "./package.json": "./package.json",
      "./browser": "./src/base/index.ts",
      "./node": "./src/node/index.ts"
    }
  }
}
